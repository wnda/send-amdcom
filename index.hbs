{{!< default}}
{{#foreach posts}}
<article>
  <a href="{{url}}">
    <h4>{{title}}</h4>
    <p>
      <time datetime="{{date format='YYYY-MM-DD'}}">{{date format='DD MMMM YYYY'}}</time>
    </p>
    <p>{{tags separator=' · ' autolink='false' limit='2'}}</p>
    <p>{{excerpt words='22'}}…</p>
  </a>
</article>
{{/foreach}}
